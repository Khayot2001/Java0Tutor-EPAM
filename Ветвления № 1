public class Main
{
	public static void main(String[] args) {
	    int a = 45, b = 45;
	    if (a + b < 180){
	        if (a != 0 && b !=0){
	            System.out.println("ДА");
	            if (a == 90 || b == 90 || a + b == 90){
	                System.out.println("Прямоугольный");   
	            }
	        }
	    }
	    else {
	        System.out.println("НЕТ");
	    }

	}
}
