public class Main
{
	public static void main(String[] args) {
	    int x = 3, y = 4;
	    
	    if (x < 5 && x > -5 && y > -4 && y < 1){
	       System.out.println("true");
	    }
	    else if (x < 3 && x > -3 && y < 5 && y > -4){
	            System.out.println("true");
	    }
	    else {
	        System.out.println("folse");
	    }
	}
}
