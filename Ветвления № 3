public class Main
{
    public static void main(String[] args) {
	    int x1 = 1, y1 = 3;
	    int x2 = 2, y2 = 2;
	    int x3 = 0, y3 = 1;
        
        if (((x3 - x1) * (y2 - y1)) == ((y3 - y1) * (x2 - x1))){
            System.out.println("Точки лежат на одной прямой");
        }
        else{
            System.out.println("НЕТ");
        }
	}
}
